{
  "title": "Remaps special key windows style",
  "rules": [
    {
      "description": "remap the - into =",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "equal_sign"
          },
          "to": [
            {
              "key_code": "slash"
            }
          ]
        }
      ]
    }, 
    {
      "description": "remap the _ into +",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "equal_sign",
            "modifiers": {
              "mandatory": ["left_shift"]
            }
          },
          "to": [
            {
              "key_code": "slash",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    }, 
    {
      "description": "remap the = into !",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "slash"
          },
          "to": [
            {
              "key_code": "8"
            }
          ]
        }
      ]
    },
    {
      "description": "remap the 8 into _",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "8"
          },
          "to": [
            {
              "key_code": "equal_sign",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "remap the 6 into -",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "6"
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ]
        }
      ]
    },
    {
      "description": "remap the â€˜ + left shift into {",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "4",
            "modifiers": {
              "mandatory": ["right_option"]
            }
          },
          "to": [
            {
              "key_code": "5",
              "modifiers": ["right_option"]
            }
          ]
        }
      ]
    },
    {
      "description": "remap the ( + left shift into [",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "5",
            "modifiers": {
              "mandatory": ["right_option"]
            }
          },
          "to": [
            {
              "key_code": "5",
              "modifiers": ["right_option", "left_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "remap the ) + left shift into ]",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "hyphen",
            "modifiers": {
              "mandatory": ["right_option"]
            }
          },
          "to": [
            {
              "key_code": "hyphen",
              "modifiers": ["right_option", "left_shift"]
            }
          ]
        }
      ]
    },
    {
      "description": "remap the = + right option into }",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "equal_sign",
            "modifiers": {
              "mandatory": ["right_option"]
            }
          },
          "to": [
            {
              "key_code": "hyphen",
              "modifiers": ["right_option"]
            }
          ]
        }
      ]
    },
    {
      "description": "remap right option + 0 to @",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "0",
            "modifiers": {
              "mandatory": ["right_option"]
            }
          },
          "to": [
            {
              "key_code": "non_us_backslash"
            }
          ]
        }
      ]
    },
    {
      "description": "remap right option + 3 to #",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "3",
            "modifiers": {
              "mandatory": ["right_option"]
            }
          },
          "to": [
            {
              "key_code": "non_us_backslash",
              "modifiers": {
                "mandatory": ["left_shift"]
              }
            }
          ]
        }
      ]
    }
  ]
}
